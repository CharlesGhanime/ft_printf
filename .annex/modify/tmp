{
	char c41 = 'c';
	unsigned int u40 = 30731;
	char c33 = 'c';
	unsigned int x32 = 6752;
	unsigned int o31 = 15370;
	unsigned int u30 = 7382;
	unsigned int o20 = 101;
	unsigned int X12 = 13195;
	unsigned int X11 = 11958;
	unsigned int o10 = 17625;
